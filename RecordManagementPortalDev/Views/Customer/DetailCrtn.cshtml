@model RecordManagementPortalDev.Models.CrtnType
@{
    ViewData["Title"] = "DetailCrtn";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h4 class="mt-3">Detail Carton Type</h4>
<div class="border p-3 mt-4">	
	<div class="row">
		<div class="col-6">
			<div class="form-group row">
				<label asp-for="CtnType" class="col-form-label mt-4 col-sm-4">Carton Type</label>
				<div class="mt-4 col-sm-8">
					<input asp-for="CtnType" type="text" class="form-control" id="CtnType" aria-describedby="Carton Type" readonly="">
				</div>
			</div>
			<div class="form-group row">
				<label asp-for="Description" class="col-form-label mt-4 col-sm-4">Description</label>
				<div class="mt-4 col-sm-8">
					<input asp-for="Description" type="text" class="form-control" id="Description" aria-describedby="Description" readonly="">
				</div>
			</div>
			<div class="form-group">
						<label class="form-label mt-4">Standard Material</label>
						<div class="form-check">
							<input class="form-check-input" type="checkbox" value="" asp-for="StdBarcode" disabled>
							<label class="form-check-label" asp-for="StdBarcode">
								Barcode Label
							</label>
						</div>
						<div class="form-check">
							<input class="form-check-input" type="checkbox" value="" asp-for="StdPlasticBag" disabled>
							<label class="form-check-label" asp-for="StdPlasticBag">
								Plastic Bag
							</label>
						</div>
						<div class="form-check">
							<input class="form-check-input" type="checkbox" value="" asp-for="StdRIC" disabled>
							<label class="form-check-label" asp-for="StdRIC">
								Record Index Card
							</label>
						</div>	
						<div class="form-check">
							<input class="form-check-input" type="checkbox" value="" asp-for="StdCableTies" disabled>
							<label class="form-check-label" asp-for="StdCableTies">
								Cable Ties
							</label>
						</div>	
						<div class="form-check">
							<input class="form-check-input" type="checkbox" value="" asp-for="StdZipBag" disabled>
							<label class="form-check-label" asp-for="StdZipBag">
								Zip Lock Bag
							</label>
						</div>	
					</div>
			<div class="form-group row">
				<label asp-for="Reorder" class="col-form-label mt-4 col-sm-4">Reorder</label>
				<div class="mt-4 col-sm-8">
					<input asp-for="Reorder" type="text" class="form-control" id="Reorder" aria-describedby="Reorder" readonly="">
				</div>
			</div>
			<div class="form-group row">
				<label asp-for="CloseBal" class="col-form-label mt-4 col-sm-4">Close Balance</label>
				<div class="mt-4 col-sm-8">
					<input asp-for="CloseBal" type="text" class="form-control" id="CloseBal" aria-describedby="Customer Address 2" readonly="">
				</div>
			</div>
			<div class="form-group row">
				<label asp-for="AsatDate" class="col-form-label mt-4 col-sm-4">AsatDate</label>
				<div class="mt-4 col-sm-8">
					<input asp-for="AsatDate" type="text" class="form-control" id="AsatDate" aria-describedby="AsatDate" readonly="">
				</div>
			</div>			
			<a class="btn btn-outline-light mt-3" href="javascript: history.go(-1)">Back To Carton Type List</a>
		</div>
		<div class="col-6">			
		</div>
	</div>
</div>



